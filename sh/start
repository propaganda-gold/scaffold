FILE_PATH=$1
FILE_TYPE=$2
REBUILD=0

BAZEL_PATH=bazel-bin/forklift/start
CACHE_PATH=~/bin-cache/start

if [ ${REBUILD} -eq 1 ]; then
	burn build forklift:start
	if [ $? -eq 0 ]; then
		echo OK
		cp -f ${BAZEL_PATH} ${CACHE_PATH}
	else
		echo FAIL
		exit 1;
	fi
fi

SH_FILE=/tmp/start.sh
${CACHE_PATH} --path ${FILE_PATH} --type ${FILE_TYPE} --dry_run=false | tee $SH_FILE
bash $SH_FILE
