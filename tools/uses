grep --exclude-dir="bazel-*" -HnR --include "*.cpp" "$1" 
grep --exclude-dir="bazel-*" -HnR --include "BUILD" "$1" 
grep --exclude-dir="bazel-*" -HnR --include "*.pbtxt" "$1" 
